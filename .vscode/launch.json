   "version": "0.2.0",
   "configurations": [
        {
            "name": ".NET Core Launch LOH Demo",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "buildLOH",
            // If you have changed target frameworks, make sure to update the program path.
            "program": "${workspaceFolder}/src/LOHAllocations/bin/Debug/netcoreapp2.1/LOHAllocations.dll",
            "args": [],
            "cwd": "${workspaceFolder}/src/LOHAllocations",
            "stopAtEntry": false,
            "internalConsoleOptions": "openOnSessionStart",
            "env": {
                "ASPNETCORE_ENVIRONMENT": "Development"
            }
        },
